{
  "dashboard": {
    "id": null,
    "title": "Log Processing System",
    "uid": "log-processing",
    "version": 1,
    "schemaVersion": 30,
    "panels": [
      {
        "id": 1,
        "title": "Events Generated per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(log_events_generated_total[1m])",
            "legendFormat": "Generation Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Events Processed per Second",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(log_events_processed_total[1m])",
            "legendFormat": "Processing Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "System Response Times",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_server_requests_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_server_requests_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
