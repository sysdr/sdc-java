{
  "dashboard": {
    "id": null,
    "title": "Log Processor System Dashboard",
    "tags": ["log-processor"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Log Ingestion Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(logs_received_total[5m])",
            "legendFormat": "Logs/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Processing Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "log_processing_duration_seconds",
            "legendFormat": "Processing Time"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(logs_rejected_total[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "legendFormat": "{{job}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {"from": "now-1h", "to": "now"},
    "refresh": "5s"
  }
}
