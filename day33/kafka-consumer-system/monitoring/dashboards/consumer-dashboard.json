{
  "dashboard": {
    "title": "Kafka Consumer Metrics",
    "panels": [
      {
        "title": "Consumer Lag",
        "targets": [
          {
            "expr": "kafka_consumer_lag"
          }
        ]
      },
      {
        "title": "Messages Processed",
        "targets": [
          {
            "expr": "rate(logs_processed_total[5m])"
          }
        ]
      },
      {
        "title": "Processing Latency",
        "targets": [
          {
            "expr": "log_processing_duration"
          }
        ]
      }
    ]
  }
}
