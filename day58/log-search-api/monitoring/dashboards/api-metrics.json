{
  "dashboard": {
    "title": "Search API Metrics",
    "panels": [
      {
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(api_search_success_total[5m])"
          }
        ]
      },
      {
        "title": "P95 Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(api_search_duration_bucket[5m]))"
          }
        ]
      },
      {
        "title": "Rate Limit Exceeded",
        "targets": [
          {
            "expr": "rate(api_rate_limit_exceeded_total[5m])"
          }
        ]
      },
      {
        "title": "Circuit Breaker State",
        "targets": [
          {
            "expr": "rate(api_circuit_breaker_activated_total[5m])"
          }
        ]
      }
    ]
  }
}
